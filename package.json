        "@types/mocha": "^10.0.1",
        "@types/node": "^16.4.7",
        "@types/sinon": "^10.0.13",
        "@types/uuid": "^9.0.1",
        "chai": "^4.3.7",
        "markdown-notes-tree": "^1.12.0",
        "mocha": "^10.2.0",
        "prettier": "^2.3.2",
